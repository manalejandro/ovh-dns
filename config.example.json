      "enabled": true
    },
    {
      "id": "ifconfig",
      "name": "ifconfig.me",
      "ipv4Url": "https://ipv4.icanhazip.com",
      "ipv6Url": "https://ipv6.icanhazip.com",
      "enabled": false
    }
  ],
  "autoUpdate": {
    "enabled": false,
    "checkInterval": 300,
    "targetDomains": []
  }
